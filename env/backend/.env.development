# ==================== Backend 配置 - 开发环境 ====================
# 此配置包含 Backend 模块独有的配置

# ==================== 服务器配置 ====================
SERVER_PORT=8080

# ==================== 后端服务URL ====================
# 后端自己的URL,供AI模块回调使用
BACKEND_BASE_URL=http://localhost:8080

# ==================== CORS配置 ====================
CORS_ORIGINS=http://localhost:3000

# ==================== 雪花算法配置 ====================
SNOWFLAKE_DATACENTER_ID=0
SNOWFLAKE_WORKER_ID=0

# ==================== 日志配置 ====================
LOG_LEVEL=DEBUG
WEB_LOG_LEVEL=INFO
SQL_LOG_LEVEL=DEBUG
SQL_PARAM_LOG_LEVEL=TRACE
SHOW_SQL=true
